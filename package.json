{
  "name": "wallpaper-changer",
  "productName": "wallpaper-changer",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "make:icon": "electron-icon-maker --input=./icon/original.png --output=./"
  },
  "keywords": [],
  "author": {
    "name": "phoeshow",
    "email": "phoeshow.me@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./icons/win/icon.ico",
        "name": "Wallpaper Changer"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "wallpaper_changer"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "devContentSecurityPolicy": "default-src 'self' 'unsafe-inline' data:; script-src 'self' 'unsafe-eval' 'unsafe-inline' data:; img-src 'self' *.wallhaven.cc blob:",
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.js"
                  }
                },
                {
                  "html": "./src/wallpaper.html",
                  "js": "./src/wallpaper_renderer.js",
                  "name": "wallpaper_window",
                  "preload": {
                    "js": "./src/wallpaper_preload.js"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@electron-forge/plugin-webpack": "6.0.0-beta.61",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.0.0",
    "electron": "16.0.4",
    "electron-icon-maker": "^0.0.5",
    "node-loader": "^2.0.0",
    "style-loader": "^3.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@phoeshow/electron-wallpaper": "^0.0.1",
    "@reduxjs/toolkit": "^1.7.0",
    "antd": "^4.17.3",
    "classnames": "^2.3.1",
    "dexie": "^3.2.0",
    "dexie-react-hooks": "^1.0.7",
    "electron-dl": "^3.3.0",
    "electron-is": "^3.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "got": "^12.0.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.1.1",
    "redux": "^4.1.2",
    "swr": "^1.1.1"
  }
}
